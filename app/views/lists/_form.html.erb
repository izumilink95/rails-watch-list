<div class="container" style="padding-top:80px;width:50vw;">
  <%= simple_form_for(@list) do |f| %>
    <div class="form-group">
      <%= f.input :name, input_html: { class: "form-control" } %>
      <%= f.input :photo, as: :file %>
      <br>
      <%= f.submit class: "btn btn-secondary", style: "color:white;" %>
    </div>
  <% end %>
</div>

